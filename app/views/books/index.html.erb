<h1>Books</h1>

<table class="table table-hover">
  <thead>
    <tr>
      <th>Title</th>
      <th>Author</th>
      <th>Price</th>
      <th>Genre</th>
    </tr>
  </thead>
  <tbody>

  <% books.each do |book| %>
    <tr>
      <td>title.book</td>
      <td>author.book</td>
      <td>price.book</td>
      <td>genre.book</td>
    </tr>
    <% end %>
  </tbody>
</table>